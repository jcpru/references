}

@article{sivadas_stacking-dependent_2018,
	title = {Stacking-{Dependent} {Magnetism} in {Bilayer} {CrI3}},
	volume = {18},
	issn = {1530-6984},
	url = {https://pubs.acs.org/doi/10.1021/acs.nanolett.8b03321},
