	urldate = {2024-03-18},
	journal = {Small},
	publisher = {John Wiley \& Sons, Ltd},
	author = {Chen, Hongyan and Yang, Hung‐Hsiang and Frauhammer, Timo and You, Haoran and Sun, Qing and Nagel, Peter and Schuppler, Stefan and Gaspar, Ana Belén and Real, José Antonio and Wulfhekel, Wulf},
	month = jun,
	year = {2023},
	keywords = {crossover molecules, exchange interaction, ferromagnetic coupling, molecular magnetism, spin},
	pages = {2300251},
}

@article{frauhammer_indirect_2021,
	title = {Indirect {Spin}-{Readout} of {Rare}-{Earth}-{Based} {Single}-{Molecule} {Magnet} with {Scanning} {Tunneling} {Microscopy}},
