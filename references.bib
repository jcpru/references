@article{biswas_pygwbse_2023,
	title = {{pyGWBSE}: a high throughput workflow package for {GW}-{BSE} calculations},
	volume = {9},
	issn = {2057-3960},
	url = {https://www.nature.com/articles/s41524-023-00976-y},
	doi = {10.1038/s41524-023-00976-y},
	abstract = {{\textless}p{\textgreater} We develop an open-source python workflow package, {\textless}italic{\textgreater}py{\textless}/italic{\textgreater} GWBSE to perform automated first-principles calculations within the GW-BSE (Bethe-Salpeter) framework. GW-BSE is a many body perturbation theory based approach to explore the quasiparticle (QP) and excitonic properties of materials. GW approximation accurately predicts bandgaps of materials by overcoming the bandgap underestimation issue of the more widely used density functional theory (DFT). BSE formalism produces absorption spectra directly comparable with experimental observations. {\textless}italic{\textgreater}py{\textless}/italic{\textgreater} GWBSE package achieves complete automation of the entire multi-step GW-BSE computation, including the convergence tests of several parameters that are crucial for the accuracy of these calculations. {\textless}italic{\textgreater}py{\textless}/italic{\textgreater} GWBSE is integrated with {\textless}italic{\textgreater}Wannier90{\textless}/italic{\textgreater} , to generate QP bandstructures, interpolated using the maximally-localized wannier functions. {\textless}italic{\textgreater}py{\textless}/italic{\textgreater} GWBSE also enables the automated creation of databases of metadata and data, including QP and excitonic properties, which can be extremely useful for future material discovery studies in the field of ultra-wide bandgap semiconductors, electronics, photovoltaics, and photocatalysis. {\textless}/p{\textgreater}},
	number = {1},
	journal = {npj Computational Materials},
	author = {Biswas, Tathagata and Singh, Arunima K.},
	month = feb,
	year = {2023},
	pages = {22},
}

