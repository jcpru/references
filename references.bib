@article{kollmar_heisenberg_1993,
	title = {A {Heisenberg} {Hamiltonian} for intermolecular exchange interaction: {Spin} delocalization and spin polarization},
	volume = {98},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/98/1/453/811592/A-Heisenberg-Hamiltonian-for-intermolecular},
	doi = {10.1063/1.464639},
	abstract = {{\textless}p{\textgreater}We present a Heisenberg Hamiltonian describing the exchange interaction between two molecular units with unpaired electrons. Each unit is represented not only by its open-shell ground state wave function, but also by an admixture of singly excited configurations which account for the spin polarization effect and may lead to negative spin densities in some regions. The states of the two units are then coupled in a valence-bond-type fashion resulting in an exchange coupling constant J expressed in terms of orbitals localized on each unit. It consists of three parts: (a) interaction of the ground state terms; (b) interaction of the spin polarization term of unit A with the ground state term of B; and (c) vice versa. The latter two contributions can be considered as an exchange interaction involving negative spin densities as has been first suggested by McConnell for the intermolecular coupling between alternant hydrocarbons. It is shown that they follow the same simple spin algebra given by the general form of a Heisenberg Hamiltonian as contribution (a) and that they also have a similar structure. The relation between the coupling constant and the spin density is discussed.{\textless}/p{\textgreater}},
	number = {1},
	journal = {The Journal of Chemical Physics},
	author = {Kollmar, Christian and Kahn, Olivier},
	month = jan,
	year = {1993},
	pages = {453--472},
}

