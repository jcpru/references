	publisher = {American Chemical Society},
	author = {Zelovich, Tamar and Hansen, Thorsten and Tuckerman, Mark E.},
	month = dec,
	year = {2022},
	keywords = {Alignment, Broadening, Molecular Energy Levels, Molecule Adsorption, Molecule/Surface Interface, Shifting},
	pages = {9854--9860},
}

