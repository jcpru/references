	publisher = {Nature Publishing Group},
	author = {Bairagi, Kaushik and Iasco, Olga and Bellec, Amandine and Kartsev, Alexey and Li, Dongzhe and Lagoute, Jérôme and Chacon, Cyril and Girard, Yann and Rousset, Sylvie and Miserque, Frédéric and Dappe, Yannick J and Smogunov, Alexander and Barreteau, Cyrille and Boillot, Marie-Laure and Mallah, Talal and Repain, Vincent},
