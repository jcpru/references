@article{harrison_photoelectrolysis_2017,
	title = {Photoelectrolysis {Using} {Type}-{II} {Semiconductor} {Heterojunctions}},
	volume = {7},
	issn = {2045-2322},
	url = {https://www.nature.com/articles/s41598-017-11971-x},
	doi = {10.1038/s41598-017-11971-x},
	abstract = {{\textless}p{\textgreater} The solar-powered production of hydrogen for use as a renewable fuel is highly desirable for the world’s future energy infrastructure. However, difficulties in achieving reasonable efficiencies, and thus cost-effectiveness, have hampered significant research progress. Here we propose the use of semiconductor nanostructures to create a type-II heterojunction at the semiconductor–water interface in a photoelectrochemical cell (PEC) and theoretically investigate it as a method of increasing the maximum photovoltage such a cell can generate under illumination, with the aim of increasing the overall cell efficiency. A model for the semiconductor electrode in a PEC is created, which solves the Schrödinger, Poisson and drift–diffusion equations self-consistently. From this, it is determined that ZnO quantum dots on bulk n-InGaN with low In content {\textless}italic{\textgreater}x{\textless}/italic{\textgreater} is the most desirable system, having electron-accepting and -donating states straddling the oxygen- and hydrogen-production potentials for {\textless}italic{\textgreater}x{\textless}/italic{\textgreater}  \&lt; 0.26, though large variance in literature values for certain material parameters means large uncertainties in the model output. Accordingly, results presented here should form the basis for further experimental work, which will in turn provide input to refine and develop the model. {\textless}/p{\textgreater}},
	number = {1},
	journal = {Scientific Reports},
	author = {Harrison, S. and Hayne, M.},
	month = sep,
	year = {2017},
	pages = {11638},
}

@article{haryono_parallel_2009,
	title = {Parallel {Crystallization} of a “{Static}” and a {Spin}‐{Crossover} {Polymorph} of an {Iron}({II}) {Complex} from the {Same} {Solution}},
	volume = {2009},
	issn = {1434-1948},
	url = {https://chemistry-europe.onlinelibrary.wiley.com/doi/10.1002/ejic.200900130},
	doi = {10.1002/ejic.200900130},
	abstract = {{\textless}p{\textgreater} The ligand 2,6‐bis(1 {\textless}italic{\textgreater}H{\textless}/italic{\textgreater} ‐pyrazol‐1‐yl)‐4‐(thiocyanatomethyl)pyridine (L) has been prepared from the hydroxymethyl precursor by OH/Br exchange and subsequent thiocyanate substitution. Two polymorphs of the unsolvated iron(II) complex [Fe(L) $_{\textrm{2}}$ ](BF $_{\textrm{4}}$ ) $_{\textrm{2}}$ crystallize at room temperature from the same solution (MeOH), and side by side. One (yellow) is high spin between 4 and 350 K, whereas the other (red‐brown) shows spin crossover, with a thermal spin‐transition centred at 272 K (width ca. 50 K). Both forms have been fully characterized by variable‐temperature structure determination and magnetic susceptibility measurements. At a given temperature, the solid‐state structures differ strongly with respect to the coordination geometry at iron(II), the orientation of the thiocyanatomethyl substituents, and cation‐anion contacts. The implications of these structural differences for the observed spin behaviour are discussed. (© Wiley‐VCH Verlag GmbH \&amp; Co. KGaA, 69451 Weinheim, Germany, 2009) {\textless}/p{\textgreater}},
	number = {14},
	journal = {European Journal of Inorganic Chemistry},
	publisher = {Wiley Online Library},
	author = {Haryono, Marco and Heinemann, Frank W. and Petukhov, Konstantin and Gieb, Klaus and Müller, Paul and Grohmann, Andreas},
	month = may,
	year = {2009},
	pages = {2136--2143},
}

@article{harzmann_singlemolecule_2015,
	title = {Single‐{Molecule} {Spin} {Switch} {Based} on {Voltage}‐{Triggered} {Distortion} of the {Coordination} {Sphere}},
	volume = {54},
	issn = {1433-7851},
	url = {https://onlinelibrary.wiley.com/doi/10.1002/anie.201505447},
	doi = {10.1002/anie.201505447},
	abstract = {{\textless}p{\textgreater} Here, we report on a new single‐molecule‐switching concept based on the coordination‐sphere‐dependent spin state of Fe $^{\textrm{II}}$ species. The perpendicular arrangement of two terpyridine (tpy) ligands within heteroleptic complexes is distorted by the applied electric field. Whereas one ligand fixes the complex in the junction, the second one exhibits an intrinsic dipole moment which senses the {\textless}italic{\textgreater}E{\textless}/italic{\textgreater} field and causes the distortion of the Fe $^{\textrm{II}}$ coordination sphere triggering the alteration of its spin state. A series of complexes with different dipole moments have been synthesized and their transport features were investigated via mechanically controlled break‐junctions. Statistical analyses support the hypothesized switching mechanism with increasing numbers of junctions displaying voltage‐dependent bistabilities upon increasing the Fe $^{\textrm{II}}$ complexes’ intrinsic dipole moments. A constant threshold value of the {\textless}italic{\textgreater}E{\textless}/italic{\textgreater} field required for switching corroborates the mechanism. {\textless}/p{\textgreater}},
	number = {45},
	journal = {Angewandte Chemie International Edition},
	author = {Harzmann, Gero D. and Frisenda, Riccardo and van der Zant, Herre S. J. and Mayor, Marcel},
	month = nov,
	year = {2015},
	keywords = {Fe terpyridine, break junction, molecular electronics, molecular switches, spin crossover, ★},
	pages = {13425--13430},
}

@article{hasyim_supervised_2022,
	title = {Supervised learning and the finite-temperature string method for computing committor functions and reaction rates},
	volume = {157},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/157/18/184111/2842057/Supervised-learning-and-the-finite-temperature},
	doi = {10.1063/5.0102423},
	abstract = {{\textless}p{\textgreater}A central object in the computational studies of rare events is the committor function. Though costly to compute, the committor function encodes complete mechanistic information of the processes involving rare events, including reaction rates and transition-state ensembles. Under the framework of transition path theory, Rotskoff et al. [Proceedings of the 2nd Mathematical and Scientific Machine Learning Conference, Proceedings of Machine Learning Research (PLMR, 2022), Vol. 145, pp. 757–780] proposes an algorithm where a feedback loop couples a neural network that models the committor function with importance sampling, mainly umbrella sampling, which collects data needed for adaptive training. In this work, we show additional modifications are needed to improve the accuracy of the algorithm. The first modification adds elements of supervised learning, which allows the neural network to improve its prediction by fitting to sample-mean estimates of committor values obtained from short molecular dynamics trajectories. The second modification replaces the committor-based umbrella sampling with the finite-temperature string (FTS) method, which enables homogeneous sampling in regions where transition pathways are located. We test our modifications on low-dimensional systems with non-convex potential energy where reference solutions can be found via analytical or finite element methods, and show how combining supervised learning and the FTS method yields accurate computation of committor functions and reaction rates. We also provide an error analysis for algorithms that use the FTS method, using which reaction rates can be accurately estimated during training with a small number of samples. The methods are then applied to a molecular system in which no reference solution is known, where accurate computations of committor functions and reaction rates can still be obtained.{\textless}/p{\textgreater}},
	number = {18},
	journal = {The Journal of Chemical Physics},
	author = {Hasyim, Muhammad R. and Batton, Clay H. and Mandadapu, Kranthi K.},
	month = nov,
	year = {2022},
	pages = {184111},
}

