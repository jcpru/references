@article{blase_gw_2016,
	title = {{GW} and {Bethe}-{Salpeter} {Study} of {Small} {Water} {Clusters}},
	volume = {144},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/144/3/034109/194320/GW-and-Bethe-Salpeter-study-of-small-water},
	doi = {10.1063/1.4940139},
	abstract = {{\textless}p{\textgreater}We study within the GW and Bethe-Salpeter many-body perturbation theories the electronic and optical properties of small (H2O)n water clusters (n = 1-6). Comparison with high-level CCSD(T) Coupled-Cluster at the Single Double (Triple) levels and ADC(3) Green’s function third order algebraic diagrammatic construction calculations indicates that the standard non-self-consistent G0W0@PBE or G0W0@PBE0 approaches significantly underestimate the ionization energy by about 1.1 eV and 0.5 eV, respectively. Consequently, the related Bethe-Salpeter lowest optical excitations are found to be located much too low in energy when building transitions from a non-self-consistent G0W0 description of the quasiparticle spectrum. Simple self-consistent schemes, with update of the eigenvalues only, are shown to provide a weak dependence on the Kohn-Sham starting point and a much better agreement with reference calculations. The present findings rationalize the theory to experiment possible discrepancies observed in previous G0W0 and Bethe-Salpeter studies of bulk water. The increase of the optical gap with increasing cluster size is consistent with the evolution from gas to dense ice or water phases and results from an enhanced screening of the electron-hole interaction.{\textless}/p{\textgreater}},
	number = {3},
	journal = {The Journal of Chemical Physics},
	author = {Blase, Xavier and Boulanger, Paul and Bruneval, Fabien and Fernandez-Serra, Marivi and Duchemin, Ivan},
	month = jan,
	year = {2016},
	pages = {034109},
}

