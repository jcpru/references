@article{chen_observation_2023,
	title = {Observation of {Exchange} {Interaction} in {Iron}({II}) {Spin} {Crossover} {Molecules} in {Contact} with {Passivated} {Ferromagnetic} {Surface} of {Co}/{Au}(111)},
