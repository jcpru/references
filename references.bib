@article{van_houten_quantum_1996,
	title = {Quantum {Point} {Contacts}},
	volume = {49},
	issn = {0031-9228},
	url = {https://pubs.aip.org/physicstoday/article/49/7/22/409107/Quantum-Point-ContactsThe-quantization-of},
	doi = {10.1063/1.881503},
	abstract = {{\textless}p{\textgreater}Punctuated equilibrium, the notion that evolution in nature is stepwise rather than continuous, sometimes applies to evolution in science as well. The seed of a scientific breakthrough may slumber for a decade or even longer without generating much interest. The seed may be a theoretical concept without clear predictions to test experimentally, or an intriguing but confusing experiment without a lucid interpretation. When the seed finally germinates, an entire field of science can reach maturity in a few years.{\textless}/p{\textgreater}},
