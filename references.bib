@article{kreutzburg_cooperativity_2017,
	title = {Cooperativity of {Spin} {Crossover} {Complexes}: {Combining} {Periodic} {Density} {Functional} {Calculations} and {Monte} {Carlo} {Simulation}},
	volume = {10},
	issn = {1996-1944},
	url = {http://www.mdpi.com/1996-1944/10/2/172},
	doi = {10.3390/ma10020172},
