@article{shimazaki_first_2009,
	title = {First principles band structure calculations based on self-consistent screened {Hartree}–{Fock} exchange potential},
	volume = {130},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/130/16/164702/904983/First-principles-band-structure-calculations-based},
	doi = {10.1063/1.3119259},
	abstract = {{\textless}p{\textgreater}A screened Hartree–Fock (HF) exchange potential with the dielectric constant was previously reported by Shimazaki and Asai [Chem. Phys. Lett. 466, 91 (2008)], in which the inverse of the dielectric constant was used to represent a fraction of the HF exchange term. In that report, the experimentally obtained value for the dielectric constant was employed. Herein, we discuss a self-consistent technique, in which the value of the dielectric constant can be automatically determined. This technique enables the energy band structure to be determined without using the experimental value. The band energy structure of diamond is calculated, a self-consistent procedure is determined to give closer bandgaps compared with the local density approximation and the generalized gradient approximation.{\textless}/p{\textgreater}},
	number = {16},
	journal = {The Journal of Chemical Physics},
	author = {Shimazaki, Tomomi and Asai, Yoshihiro},
