@article{parrish_communication_2015,
	title = {Communication: {Practical} intramolecular symmetry adapted perturbation theory via {Hartree}-{Fock} embedding},
	volume = {143},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/143/5/051103/193971/Communication-Practical-intramolecular-symmetry},
	doi = {10.1063/1.4927575},
	abstract = {{\textless}p{\textgreater}We develop a simple methodology for the computation of symmetry-adapted perturbation theory (SAPT) interaction energy contributions for intramolecular noncovalent interactions. In this approach, the local occupied orbitals of the total Hartree-Fock (HF) wavefunction are used to partition the fully interacting system into three chemically identifiable units: the noncovalent fragments A and B and a covalent linker C. Once these units are identified, the noninteracting HF wavefunctions of the fragments A and B are separately optimized while embedded in the HF wavefunction of C, providing the dressed zeroth order wavefunctions for A and B in the presence of C. Standard two-body SAPT (particularly SAPT0) is then applied between the relaxed wavefunctions for A and B. This intramolecular SAPT procedure is found to be remarkably straightforward and efficient, as evidenced by example applications ranging from diols to hexaphenyl-ethane derivatives.{\textless}/p{\textgreater}},
	number = {5},
	journal = {The Journal of Chemical Physics},
