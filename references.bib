@article{martino_preferential_2020,
	title = {Preferential out-of-plane conduction and quasi-one-dimensional electronic states in layered {1T}-{TaS2}},
	volume = {4},
	issn = {2397-7132},
	url = {https://www.nature.com/articles/s41699-020-0145-z},
	doi = {10.1038/s41699-020-0145-z},
	abstract = {{\textless}p{\textgreater} Layered transition metal dichalcogenides (TMDs) are commonly classified as quasi-two-dimensional materials, meaning that their electronic structure closely resembles that of an individual layer, which results in resistivity anisotropies reaching thousands. Here, we show that this rule does not hold for 1T-TaS $_{\textrm{2}}$ —a compound with the richest phase diagram among TMDs. Although the onset of charge density wave order makes the in-plane conduction non-metallic, we reveal that the out-of-plane charge transport is metallic and the resistivity anisotropy is close to one. We support our findings with ab initio calculations predicting a pronounced quasi-one-dimensional character of the electronic structure. Consequently, we interpret the highly debated metal-insulator transition in 1T-TaS $_{\textrm{2}}$ as a quasi-one-dimensional instability, contrary to the long-standing Mott localisation picture. In a broader context, these findings are relevant for the newly born field of van der Waals heterostructures, where tuning interlayer interactions (e.g., by twist, strain, intercalation, etc.) leads to new emergent phenomena. {\textless}/p{\textgreater}},
	number = {1},
	journal = {npj 2D Materials and Applications},
	author = {Martino, E. and Pisoni, A. and Ćirić, L. and Arakcheeva, A. and Berger, H. and Akrap, A. and Putzke, C. and Moll, P. J. W. and Batistić, I. and Tutiš, E. and Forró, L. and Semeniuk, K.},
	month = may,
	year = {2020},
	pages = {7},
}

