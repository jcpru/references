@article{li_density_2018,
	title = {Density functional theory of electron transfer beyond the {Born}-{Oppenheimer} approximation: {Case} study of {LiF}},
	volume = {148},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/148/8/084110/196610/Density-functional-theory-of-electron-transfer},
	doi = {10.1063/1.5011663},
	abstract = {{\textless}p{\textgreater}We perform model calculations for a stretched LiF molecule, demonstrating that nonadiabatic charge transfer effects can be accurately and seamlessly described within a density functional framework. In alkali halides like LiF, there is an abrupt change in the ground state electronic distribution due to an electron transfer at a critical bond length R = Rc, where an avoided crossing of the lowest adiabatic potential energy surfaces calls the validity of the Born-Oppenheimer approximation into doubt. Modeling the R-dependent electronic structure of LiF within a two-site Hubbard model, we find that nonadiabatic electron-nuclear coupling produces a sizable elongation of the critical Rc by 0.5 bohr. This effect is very accurately captured by a simple and rigorously derived correction, with an M−1 prefactor, to the exchange-correlation potential in density functional theory, M = reduced nuclear mass. Since this nonadiabatic term depends on gradients of the nuclear wave function and conditional electronic density, ∇Rχ(R) and ∇Rn(r, R), it couples the Kohn-Sham equations at neighboring R points. Motivated by an observed localization of nonadiabatic effects in nuclear configuration space, we propose a local conditional density approximation—an approximation that reduces the search for nonadiabatic density functionals to the search for a single function y(n).{\textless}/p{\textgreater}},
	number = {8},
	journal = {The Journal of Chemical Physics},
	author = {Li, Chen and Requist, Ryan and Gross, E. K. U.},
	month = feb,
	year = {2018},
	pages = {084110},
}

