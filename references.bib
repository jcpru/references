}

@article{becke_density-functional_1993,
	title = {Density-functional thermochemistry. {III}. {The} role of exact exchange},
	volume = {98},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/98/7/5648/842114/Density-functional-thermochemistry-III-The-role-of},
	doi = {10.1063/1.464913},
	abstract = {{\textless}p{\textgreater}Despite the remarkable thermochemical accuracy of Kohn–Sham density-functional theories with gradient corrections for exchange-correlation [see, for example, A. D. Becke, J. Chem. Phys. 96, 2155 (1992)], we believe that further improvements are unlikely unless exact-exchange information is considered. Arguments to support this view are presented, and a semiempirical exchange-correlation functional containing local-spin-density, gradient, and exact-exchange terms is tested on 56 atomization energies, 42 ionization potentials, 8 proton affinities, and 10 total atomic energies of first- and second-row systems. This functional performs significantly better than previous functionals with gradient corrections only, and fits experimental atomization energies with an impressively small average absolute deviation of 2.4 kcal/mol.{\textless}/p{\textgreater}},
	number = {7},
	journal = {The Journal of Chemical Physics},
