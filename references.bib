@article{vijaykumar_multiscale_2017,
	title = {Multiscale simulations of anisotropic particles combining molecular dynamics and {Green}’s function reaction dynamics},
	volume = {146},
	issn = {0021-9606},
	url = {https://pubs.aip.org/jcp/article/146/11/114106/195022/Multiscale-simulations-of-anisotropic-particles},
	doi = {10.1063/1.4977515},
	abstract = {{\textless}p{\textgreater}The modeling of complex reaction-diffusion processes in, for instance, cellular biochemical networks or self-assembling soft matter can be tremendously sped up by employing a multiscale algorithm which combines the mesoscopic Green’s Function Reaction Dynamics (GFRD) method with explicit stochastic Brownian, Langevin, or deterministic molecular dynamics to treat reactants at the microscopic scale [A. Vijaykumar, P. G. Bolhuis, and P. R. ten Wolde, J. Chem. Phys. 143, 214102 (2015)]. Here we extend this multiscale MD-GFRD approach to include the orientational dynamics that is crucial to describe the anisotropic interactions often prevalent in biomolecular systems. We present the novel algorithm focusing on Brownian dynamics only, although the methodology is generic. We illustrate the novel algorithm using a simple patchy particle model. After validation of the algorithm, we discuss its performance. The rotational Brownian dynamics MD-GFRD multiscale method will open up the possibility for large scale simulations of protein signalling networks.{\textless}/p{\textgreater}},
	number = {11},
	journal = {The Journal of Chemical Physics},
	author = {Vijaykumar, Adithya and Ouldridge, Thomas E. and ten Wolde, Pieter Rein and Bolhuis, Peter G.},
	month = mar,
	year = {2017},
	keywords = {★},
	pages = {114106},
}

@article{marrows_neuromorphic_2024,
